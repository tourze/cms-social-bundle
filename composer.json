{
  "name": "tourze/cms-social-bundle",
  "description": "A Symfony bundle providing social features for CMS systems, including comment management and content sharing functionality.",
  "license": "MIT",
  "require": {
    "doctrine/collections": "^2.3",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^4.1",
    "easycorp/easyadmin-bundle": "^4",
    "knplabs/knp-menu": "^3.7",
    "symfony/cache-contracts": "^3",
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/doctrine-bridge": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/routing": "^7.3",
    "symfony/security-bundle": "^7.3",
    "symfony/security-core": "^7.3",
    "symfony/security-http": "^7.3",
    "symfony/serializer": "^7.3",
    "symfony/yaml": "^7.3",
    "tourze/arrayable": "1.*",
    "tourze/bundle-dependency": "1.*",
    "tourze/cms-bundle": "0.0.*",
    "tourze/doctrine-async-insert-bundle": "0.1.*",
    "tourze/doctrine-ip-bundle": "0.0.*",
    "tourze/doctrine-snowflake-bundle": "0.1.*",
    "tourze/doctrine-timestamp-bundle": "0.0.*",
    "tourze/doctrine-user-bundle": "0.0.*",
    "tourze/easy-admin-menu-bundle": "0.1.*",
    "tourze/json-rpc-core": "0.0.*",
    "tourze/json-rpc-lock-bundle": "0.1.*",
    "tourze/json-rpc-log-bundle": "0.1.*",
    "tourze/symfony-dependency-service-loader": "1.*"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^4.0",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "symfony/phpunit-bridge": "^7.3",
    "tourze/doctrine-resolve-target-entity-bundle": "1.*",
    "tourze/phpunit-doctrine-entity": "1.*",
    "tourze/phpunit-symfony-kernel-test": "0.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*",
    "tourze/phpunit-symfony-web-test": "0.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\CmsSocialBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\CmsSocialBundle\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1",
    "test": "phpunit"
  }
}